{
  "name": "3ondb-admin",
  "version": "1.0.0",
  "description": "3ONDB Admin Dashboard - Web UI for monitoring and managing the 3ON ecosystem",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "echo 'Server build complete'",
    "build:client": "cd client && npm run build",
    "start": "node server/index.js",
    "install:client": "cd client && npm install",
    "setup": "npm install && npm run install:client"
  },
  "keywords": ["3ondb", "admin", "dashboard", "monitoring"],
  "author": "3ONCOIN",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
